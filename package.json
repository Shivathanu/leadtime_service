{
  "name": "leadtimeservice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "development": "set NODE_ENV=development&&nodemon app.js",
    "quality": "set NODE_ENV=quality&&node app.js",
    "production": "set NODE_ENV=production&&node app.js",
    "migrate": "sequelize db:migrate",
    "migrate-development": "set NODE_ENV=development&&sequelize db:migrate",
    "migrate-quality": "set NODE_ENV=quality&&sequelize db:migrate",
    "migrate-production": "set NODE_ENV=production&&sequelize db:migrate",
    "migrate-test": "set NODE_ENV=test&&sequelize db:migrate",
    "test": "set NODE_ENV=test&&istanbul cover --handle-sigint ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register",
    "jshint": "jshint ."
  },
  "dependencies": {
    "async": "^2.6.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "lodash": "^4.17.10",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.7",
    "request": "^2.87.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "tedious": "^2.3.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^11.7.3",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "jshint"
  ]
}
